{
  "private": true,
  "name": "force-vue-awakens",
  "version": "1.0.0",
  "description": "Every generation has a story",
  "scripts": {
    "test": "npm run unit && npm run e2e",
    "unit": "jest",
    "e2e": "wdio",
    "build": "webpack",
    "start": "webpack-dev-server",
    "lint": "eslint src test wdio.conf.js"
  },
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "dependencies": {
    "unfetch": "^4.1.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@appjumpstart/launch": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.16",
    "@wdio/cli": "^5.7.10",
    "@wdio/jasmine-framework": "^5.7.8",
    "@wdio/junit-reporter": "^5.7.8",
    "@wdio/local-runner": "^5.7.9",
    "@wdio/selenium-standalone-service": "^5.7.8",
    "@wdio/spec-reporter": "^5.7.9",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "eslint-plugin-webdriverio": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "node": "^11.13.0",
    "url-loader": "^1.0.1",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.2.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "testURL": "http://localhost:8080",
    "transform": {
      ".+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "jest/globals": true,
      "webdriverio/wdio": true
    },
    "extends": [
      "plugin:vue/recommended",
      "standard"
    ],
    "plugins": [
      "vue",
      "jest",
      "webdriverio"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "no-unused-expressions": 0
    }
  }
}
